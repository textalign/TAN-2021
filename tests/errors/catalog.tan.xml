      <source xmlns="tag:textalign.net,2015:ns" xml:id="test-1">
         <IRI>tag:textalign.net,2015:error-test</IRI>
         <name>error tan-t</name>
         <location accessed-when="2020-02-27"
                   href="error-test-1.tan-t.xml"
                   orig-href="error-test-1.tan-t.xml"/>
      </source>
      <source xmlns="tag:textalign.net,2015:ns" xml:id="test-4">
         <IRI>tag:textalign.net,2015:error-test-4</IRI>
         <name>Test 4</name>
         <location accessed-when="2019-01-23"
                   href="error-test-4.tan-tei.xml"
                   orig-href="error-test-4.tan-tei.xml"/>
      </source>
      <source xmlns="tag:textalign.net,2015:ns" xml:id="test-8">
         <IRI>tag:textalign.net,2015:error-test-8</IRI>
         <name>error test 8 tan-t</name>
         <location accessed-when="2019-02-20"
                   href="error-test-8.tan-t.xml"
                   orig-href="error-test-8.tan-t.xml"/>
      </source>
      <adjustments xmlns="tag:textalign.net,2015:ns" src="test-1">
            <!-- cl215 seq05 -->
         <skip n="bad - worse"/>
         <!-- dty01 -->
         <skip div-type="chapter"/>
         <!-- cl215 cl203 -->
         <rename n="bad" new="bad"/>
         <!-- cl207 -->
         <equate n="bill ball"/>
      </adjustments>
      <adjustments xmlns="tag:textalign.net,2015:ns" src="test-4">
         <skip ref="2"/>
         <!-- cl214 -->
         <skip ref="2"/>
         <!-- cl213 cl215 -->
         <rename n="huh" by="4"/>
         <!-- ref01 cl216 -->
         <rename ref="7.4-6" new="8.2"/>
         <!-- cl218 -->
         <rename ref="2" by="4"/>
         <rename ref="preface" new="pref1"/>
         <!-- cl212 -->
         <rename ref="preface" new="pref3"/>
         <!-- cl206 -->
         <rename n="preface" new="pref2"/>
         <!-- cl217 -->
         <rename ref="test3" new="3"/>
         <!-- rea03 -->
         <rename n="test4" new="test5"/>
         <!-- cl205 -->
         <equate n="3 iii"/>
         <!-- cl204 -->
         <equate n="test4 test6"/>
         <reassign>
                <!-- rea04 rea02 (because tokenization will still be tried on nonleaf divs) -->
            <passage ref="2a">
               <from-tok pos="1"/>
               <through-tok pos="1"/>
            </passage>
            <!-- rea02 -->
            <passage ref="2a.3">
               <from-tok pos="2"/>
               <!-- rea01 -->
               <through-tok pos="1"/>
            </passage>
            <!-- rea03 -->
            <passage ref="test5">
               <from-tok val="Test"/>
               <through-tok pos="2"/>
            </passage>
            <!-- rea02 -->
            <passage ref="test5">
               <from-tok val="Plus"/>
               <through-tok val="another"/>
            </passage>
            <!-- rea02 -->
            <passage ref="test5">
               <from-tok val="another"/>
               <through-tok pos="last"/>
            </passage>
            <to ref="3"/>
         </reassign>
      </adjustments>
   </doc>
